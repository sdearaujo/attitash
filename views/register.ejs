<!doctype html>
<html>
  
  <head>
    <script src="/bootstrap/docs/assets/js/jquery.min.js"></script>
    <script src="/bootstrap/docs/assets/js/bootstrap.min.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
    <link rel="stylesheet" href="/bootstrap/docs/assets/css/bootstrap.css">
    <link rel="stylesheet" href="/bootstrap/docs/assets/css/bootstrap-responsive.css"> 
    <link href="/stylesheets/register.css" rel="stylesheet" type="text/css">
    <link href="/stylesheets/attitash.css" rel="stylesheet">
    <title><%= title %></title>
  </head>
  
  <body>
    <div id="registerBackground" class="well">
      <div id="myTabContent" class="tab-content">
        <div class="tab-pane active in" id="home">
          <h1>Register</h1>
          <form method="post" action="/register" id="tab">
            <label>First Name</label>
            <input name="fname" type="text" value="Test" class="input-xlarge">
            <label>Last Name</label>
            <input name="lname" type="text" value="User" class="input-xlarge">
            <label>Username</label>
            <input name="username" type="text" value="TestUser" class="input-xlarge">
            <label>Password</label>
            <input name="password" type="password" value="password" class="input-xlarge">
            <label>Confirm Password</label>
            <input type="password" value="password" class="input-xlarge">
            <label>Email</label>
            <input name="email" type="email" value="test-user@attitash.com" class="input-xlarge">
            <div id="buttonLine">
              <button class="btn btn-primary">Submit</button>
            </div>
          </form>
        </div>
        <div class="tab-pane fade" id="profile">
          <form id="tab2" style="">
            <label>New Password</label>
            <input type="password" class="input-xlarge">
            <div>
              <button class="btn btn-primary">Update</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </body>

</html>
