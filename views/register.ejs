  <body>
    <div id="registerBackground" class="well">
      <div id="myTabContent" class="tab-content">
        <div class="tab-pane active in" id="home">
          <h1>Register</h1>
          <form id="tab">
            <label>First Name</label>
            <input type="text" value="John" class="input-xlarge">
            <label>Last Name</label>
            <input type="text" value="Jacob Jingleheimer" class="input-xlarge">
            <label>Username</label>
            <input type="text" value="johnjacobjingleheimer" class="input-xlarge">
            <label>Password</label>
            <input type="password" value="Password" class="input-xlarge">
            <label>Confirm Password</label>
            <input type="password" value="Password" class="input-xlarge">
            <label>Email</label>
            <input type="email" value="john@jingleheimer.com" class="input-xlarge">
            <div id="buttonLine">
              <button class="btn btn-primary" href="/">Submit</button>
	      <button class="btn btn-primary" href="/">Cancel</button>
            </div>
          </form>
        </div>
        <div class="tab-pane fade" id="profile">
          <form id="tab2" style="">
            <label>New Password</label>
            <input type="password" class="input-xlarge">
            <div>
              <button class="btn btn-primary">Update</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </body>

